{
  "expo": {
    "name": "Shareceipt",
    "slug": "shareceipt",
    "description": "Simple Android app to help scan and fairly split a receipt",
    "githubUrl": "https://github.com/AC-Dap/shareceipt",
    "version": "2.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2979ff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-foreground.png",
        "monochromeImage": "./assets/icon-monochrome.png",
        "backgroundImage": "./assets/icon-background.png"
      },
      "package": "com.acdap.shareceipt"
    }
  }
}
